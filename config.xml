<?xml version='1.0' encoding='utf-8'?>
<widget id="amazing-chat" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Ultimate chat app</name>
    <description>Amazing chat app</description>
    <author>
		Micha≈Ç Kobiec and Anita Pietrzyk
	</author>
    <platform name="android" />
    <plugin name="cordova-plugin-device" spec="~1.1.4" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <gap:plugin name="cordova-plugin-camera" source="npm" version="2.4.1" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="1.1.0" />
    <access launch-external="yes" origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <plugin name="cordova-plugin-downloadmanager" spec="https://github.com/vasani-arpit/cordova-plugin-downloadmanager" />
</widget>
